      <FlowPane hgap="25.0" prefHeight="110.0" prefWidth="866.0" vgap="10.0" BorderPane.alignment="CENTER">
         <children>
            <TextField fx:id="textfield1" onAction="#handleConvert" prefHeight="25.0" prefWidth="149.0" />
            <ComboBox fx:id="combobox1" prefWidth="150.0" />
            <TextField fx:id="textfield2" onAction="#handleConvert" prefHeight="25.0" prefWidth="149.0" />
            <ComboBox fx:id="combobox2" prefWidth="150.0" />
            <Button mnemonicParsing="false" onAction="#handleConvert" prefHeight="25.0" prefWidth="66.0" text="Convert" />
            <Button mnemonicParsing="false" onAction="#handleClear" prefHeight="25.0" text=" Clear" />
         </children>
         <padding>
            <Insets bottom="15.0" left="10.0" right="10.0" top="15.0" />
         </padding>
      </FlowPane>
   </center>
</BorderPane>
